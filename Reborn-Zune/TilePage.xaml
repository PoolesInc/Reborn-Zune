<Page
    x:Class="Reborn_Zune.TilePage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:behaviors="using:Microsoft.Toolkit.Uwp.UI.Animations.Behaviors"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:interactivity="using:Microsoft.Xaml.Interactivity"
    xmlns:local="using:Reborn_Zune"
    xmlns:controls="using:Reborn_Zune.Control"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid x:Name="rootPanel" Background="DarkGray">
        <controls:VariableSizedWrapPanel
            x:Name="panel"
            Width="2580"
            Height="1200"
            Margin="-20,-20,-1784,-100"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            IsComposing="True"
            ItemHeight="75"
            ItemWidth="75"
            Loaded="rootPanel_Loaded"
            Orientation="Vertical" />
    </Grid>
</Page>
